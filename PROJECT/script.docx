    #config.window_hide_transition = dissolve
    #config.window_show_transition = dissolve
    left2 = Position(xalign = -0.4, yalign = 0)
    right2 = Position(xalign = 1.7, yalign = 0)
    center2 = Position(xalign = 0.6, yalign = 0)

# Игра начинается здесь.
label start:
    scene bg home
    with fade

    $ name = renpy.input("Введите свое имя")

    show nika1 at right2
    show liza1 at left2
    n "Ну вот и наступил этот прекрасный день - твой первый день в Лицее НИУ ВШЭ!"
    l "Нам очень приятно с тобой познакомиться, [name]. Твоими помощниками на сегодня будем мы"
    hide nika1
    hide liza1
    show nika2 at right2
    show liza2 at left2
    l "Для начала предлагаю выбрать тебе класс."
    hide nika2
    hide liza2
    #menu:
    #    "Для начала нужно узнать про направления в 9 и 10 классе.":
    #
    #        "Информация про 9 класс"
    jump choice
    #menu:
    #    "Какой класс ты выберешь?"
    #    "9 класс":
    #        jump nine_class
    #
    #    "10 класс / 11 класс":
    #        jump ten
    return

label choice:
    menu:
        "Какой класс ты выберешь?"
        "9 класс":
            jump nine_class

        "Посмотреть информацию про направления 9-ого класса":
            jump inf9

        "10 класс / 11 класс":
            jump ten

        "Посмотреть информацию про направления 10-ого класса":
            jump inf10
    return

label inf9:
    show liza2 at center2
    l "В 9 классе два направления: универсальное и математичексое."
    l "«Футуритет» (9 класс) – направление будущего образования и пространство самоопределения."
    l "В течение года ученики знакомятся с направлениями обучения 10-х классов, участвуют в мероприятиях направлений, выступают с проектными и исследовательскими инициативами."
    hide liza2
    show liza1 at center2
    l "Один из важных этапов – подготовка и презентация программы дальнейшего образования на приоритетных направлениях обучения."
    hide liza1
    jump choice
    return

label inf10:
    show nika1 at center2
    n "В 10 классе десять направлений."
    n "Лицеист выбирает профильные учебные предметы (курсы) направления: учебные предметы углубленного уровня, элективные и факультативные курсы,"
    n "А также набор курсов образовательной программы факультетского дня, по которой учится один день в неделю в Вышке."
    hide nika1
    jump choice
    return

